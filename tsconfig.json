{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": ".",
    "types": ["node"],
    "noEmit": false
  },
  "exclude": [
    "./node_modules",
    "**/*.test.ts",
    "cypress/**/*.ts",
    "cypress.config.ts"
  ]
}
