{
  "name": "sudokuru-frontend",
  "version": "1.0.0",
  "main": "./app/App.tsx",
  "scripts": {
    "prepare": "husky install",
    "start": "expo start --clear",
    "start:tunnel": "expo start --tunnel --clear",
    "android": "expo start --android --clear",
    "ios": "expo start --ios --clear",
    "web": "npx expo start --web --clear",
    "web:dev": "cross-env ENV=development npx expo start --web --clear",
    "build:metro-web": "npx expo export --platform web",
    "preview:vercel": "vercel --name sudokuru-dev --yes",
    "prettier": "npx prettier --write .",
    "open:cypress": "npx cypress open",
    "test:cypress": "npx cypress run --config video=false,screenshotOnRunFailure=false",
    "test:unit": "jest",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "expo:check": "npx expo install --check",
    "snyk:opensource": "npx snyk test --all-projects --exclude=.vercel --org=sudokuru",
    "snyk:code": "npx snyk code test --org=sudokuru"
  },
  "lint-staged": {
    "**/*": "npx prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/metro-config": "~0.10.0",
    "@expo/metro-runtime": "^1.1.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "expo": "49.0.21",
    "expo-status-bar": "~1.6.0",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.0",
    "react-native-awesome-alerts": "^2.0.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.5.2",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "sudokuru": "2.1.0",
    "expo-updates": "~0.18.17"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@cypress/code-coverage": "^3.11.0",
    "@tauri-apps/cli": "^1.5.5",
    "@types/jest": "^29.5.4",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.70.13",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-expo": "^49.0.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^14.0.1",
    "metro": "^0.76.6",
    "prettier": "2.8.8",
    "snyk": "^1.1259.0",
    "typescript": "^5.1.3",
    "vercel": "^28.18.1"
  },
  "private": true
}
